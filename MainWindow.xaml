<Window x:Class="IbragimovD_Autoservice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IbragimovD_Autoservice"
        mc:Ignorable="d"
        Title="IbragimovD_Autoservice" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="./images/service_logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75" />
            <RowDefinition Height="*" />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <Image Source="./images/service_logo.png" HorizontalAlignment="Left"></Image>

        <TextBlock Text="Услуги Автосервис" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Back" Name="BtnBack" HorizontalAlignment="Right"
                Click="BtnBack_Click"></Button>
    </Grid>
</Window>
